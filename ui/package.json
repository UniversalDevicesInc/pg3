{
  "name": "pg3-frontend",
  "version": "3.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --host polyglot --ssl true --ssl-key ~/.polyglot/ssl/polyglot_private.key --ssl-cert ~/.polyglot/ssl/polyglot.crt",
    "start:local": "ng serve --ssl true --ssl-key ~/.polyglot/ssl/polyglot_private.key --ssl-cert ~/.polyglot/ssl/polyglot.crt",
    "start:devhttp": "ng serve --host polyglot",
    "patchrelease": "npm version patch",
    "prerelease": "npm version prerelease",
    "postversion": "git push && git push --tags",
    "update:check": "ncu",
    "update:run": "ncu -u; npm install",
    "build:test": "ng build --configuration=test",
    "build:prod": "time ng build --prod",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/common": "9.1.12",
    "@angular/compiler": "9.1.12",
    "@angular/compiler-cli": "9.1.12",
    "@angular/core": "9.1.12",
    "@angular/forms": "9.1.12",
    "@angular/localize": "9.1.12",
    "@angular/platform-browser": "9.1.12",
    "@angular/platform-browser-dynamic": "9.1.12",
    "@angular/platform-server": "9.1.12",
    "@angular/router": "9.1.12",
    "@angular/upgrade": "9.1.12",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@ng-bootstrap/ng-bootstrap": "5.1.4",
    "angular-in-memory-web-api": "0.9.0",
    "angular2-flash-messages": "^3.0.1",
    "buffer": "^5.4.3",
    "core-js": "3.4.8",
    "file-saver": "2.0.2",
    "mqtt": "4.1.0",
    "ngx-bootstrap": "5.2.0",
    "ngx-toastr": "^13.0.0",
    "randomstring": "^1.1.5",
    "rxjs": "6.5.4",
    "setimmediate": "^1.0.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.11",
    "@angular/cli": "9.1.11",
    "@types/node": "^12.11.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "~3.7.5"
  }
}
